<Window x:Class="Estilos_de_Antonio.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Estilos_de_Antonio"
        mc:Ignorable="d"
        Title="Estilos de Antonio" Height="450" Width="800">
    <Grid Style="{StaticResource AparienciaGrid}">
        <StackPanel Style="{StaticResource AparienciaStackPanel}">
            <Image Style="{StaticResource ImagenLogo}"></Image>
            <Button Style="{StaticResource AparienciaBtnNavegacion}">Partidos</Button>
        </StackPanel>
    </Grid>
</Window>
